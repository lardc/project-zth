<pages:CommonPage x:Class="Zth.Pages.CalculationZth"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
            xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            xmlns:pages="clr-namespace:Zth.Pages"
            xmlns:vm="clr-namespace:Zth.VM" Loaded="CommonPage_Loaded"
            mc:Ignorable="d" d:DataContext="{d:DesignInstance vm:CommonVM }" 
            xmlns:res="clr-namespace:Zth.Properties"
            xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
            xmlns:components="clr-namespace:Zth.Components" Unloaded="CommonPage_Unloaded"
            d:DesignHeight="1024" d:DesignWidth="1280"
            Title="RthPulseSequence">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>

        <components:Chart Grid.Row="0"/>
        <components:BottomCheckBoxsAndTable HorizontalAlignment="Center" Grid.Row="1" />


    </Grid>
</pages:CommonPage>
